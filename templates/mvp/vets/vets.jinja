{% extends "base.jinja" %}
{% block aside %}
    <a href="/vets/new-veterian"><h2>New Veterian</h2></a>
{% endblock aside %}

{% block content %}
    <h2>All Vets</h2>
    <ul class="list-of-people">
        {% for veterian in display_vets %}
            <li>
                <a href="/vets/{{veterian.id}}">
                        <h2>{{veterian.full_name()}}</h2>
                </a>
            </li>
        {% endfor %}
    </ul>
{% endblock content %}